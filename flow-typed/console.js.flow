'use strict';

// $FlowFixMe[unsupported-syntax]
declare module 'console' {
  declare export class Console {
    constructor(stdout: unknown, stderr: unknown): void;
    assert(
      value: unknown,
      message?: string,
      ...messageParts: Array<unknown>
    ): void;
    dir(data: unknown, options?: Object): void;

    error(msg: string, ...msgParts: Array<unknown>): void;
    error(data: unknown): void;

    info(msg: string, ...msgParts: Array<unknown>): void;
    info(data: unknown): void;

    log(msg: string, ...msgParts: Array<unknown>): void;
    log(data: unknown): void;

    time(label: string): void;
    timeEnd(label: string): void;
    trace(msg: string, ...msgParts: Array<unknown>): void;

    warn(msg: string, ...msgParts: Array<unknown>): void;
    warn(data: unknown): void;
  }
}
