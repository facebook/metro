    "metro-cache": "0.82.0",
    "metro-core": "0.82.0",
    "metro-runtime": "0.82.0"
  },
  "devDependencies": {
    "@types/connect": "^3.4.35",
    "metro-babel-register": "0.82.0",
    "pretty-format": "^29.7.0"
  },
  "engines": {
    "node": ">=18.18"
  }
